	<bean class="fr.inria.viser.ad.visitor2.visitor.TokenIsWithdrawVisitor" />
	<bean
		class="fr.inria.viser.ad.visitor2.visitor.TokenTakeOfferedTokensVisitor" />
	<bean class="fr.inria.viser.ad.visitor2.visitor.TokenTransfertVisitor" />
